<?xml version="1.0" encoding="UTF-8"?>

<tobago-config
    xmlns="http://myfaces.apache.org/tobago/tobago-config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://myfaces.apache.org/tobago/tobago-config http://myfaces.apache.org/tobago/tobago-config-5.0.xsd"
    version="5.0">

  <name>astro</name>

  <theme-config>
    <default-theme>astro</default-theme>
  </theme-config>

  <content-security-policy mode="off">
    <!--<content-security-policy mode="report-only">-->
    <!--<directive>child-src 'self'</directive>-->
    <!--<directive name="img-src">'self' schnurpfeil.de</directive>-->
    <!--<directive name="script-src">unsafe-eval</directive> &lt;!&ndash; XXX funktionioert noch nicht &ndash;&gt;-->

  </content-security-policy>

  <theme-definitions>
    <theme-definition>
      <name>astro</name>
      <display-name>Astro Theme</display-name>
      <fallback>roxborough</fallback>
      <versioned>false</versioned>
      <resources production="true">
        <includes>
          <script name="/katex/katex.min.js"/>
          <style name="/katex/katex.min.css"/>
        </includes>
      </resources>
      <resources production="false">
        <includes>
          <script name="/katex/katex.js"/>
          <style name="/katex/katex.css"/>
        </includes>
      </resources>
    </theme-definition>
  </theme-definitions>


</tobago-config>
